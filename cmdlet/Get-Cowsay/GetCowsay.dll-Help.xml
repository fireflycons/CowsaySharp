      <command:example>
        <maml:title>--------------------------  Example 9  --------------------------</maml:title>
        <maml:introduction>
          <maml:paragraph>PS C:\&gt;</maml:paragraph>
        </maml:introduction>
        <dev:code>
          $message = @&quot;
          _ _ _          _   _     _
          | (_) | _____  | |_| |__ (_)___
          | | | |/ / _ \ | __| &apos;_ \| / __|
